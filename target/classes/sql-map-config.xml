<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	<typeAliases>
		<typeAlias type="com.green.biz.dto.MemberVO" alias="member"/>
		<typeAlias type="com.green.biz.dto.MypageVO" alias="mypage"/>
		<typeAlias type="com.green.biz.dto.DumbellVO" alias="dumbell"/>
		<typeAlias type="com.green.biz.dto.AdminVO" alias="Admin"/>
	</typeAliases>
	
    <!-- SQL 명령어들이 저장된 SQL 파일 등록 -->
	<mappers>
		<mapper resource="mappings/member-mapping.xml"/>
		<mapper resource="mappings/mypage-mapping.xml"/>
		<mapper resource="mappings/exercise-mapping.xml"/>
		<mapper resource="mappings/admin-mapping.xml"/>
	</mappers>
	
</configuration>
